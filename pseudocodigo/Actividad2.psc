Proceso Promedio3Notas
	Definir n1,n2,n3,promedio Como Real;
	Escribir "digite el primer numero";
	leer n1;
	Escribir "digite el segundo numero";
	leer n2;
	Escribir "digite el tercer numero";
	leer n3;
	
	promedio<-(n1+n2+n3)/3;
	
	Escribir "el promedio de las tres notas es;" ,promedio;
	
FinProceso
